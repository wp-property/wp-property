{
  "name": "usabilitydynamics/wp-property",
  "version": "1.42.0",
  "type": "wordpress-plugin",
  "homepage": "http://github.com/UsabilityDynamics/wp-property",
  "keywords": [
    "real-estate",
    "wp-property"
  ],
  "minimum-stability": "dev",
  "repositories": [
    {
      "type": "composer",
      "url": "http://repository.usabilitydynamics.com"
    }
  ],
  "require": {
    "php": ">=5.3",
    "nitra/php-min": "dev-master",
    "usabilitydynamics/lib-installer": "master",
    "usabilitydynamics/lib-api": "master",
    "usabilitydynamics/lib-ui": "master",
    "usabilitydynamics/lib-module": "master",
    "usabilitydynamics/lib-model": "master",
    "wp-property/editor": "master",
    "wp-property/admin-tools": "master",
    "wp-property/exporter": "master",
    "wp-property/default-theme": "master",
    "wp-property/list-tables": "master",
    "wp-property/sample-module": "master"
  },
  "available-modules": {
    "wp-property/advanced-rewrites": "master",
    "wp-property/agents": "master",
    "wp-property/api-manager": "master",
    "wp-property/availability": "master",
    "wp-property/bookings": "master",
    "wp-property/dynamic-templates": "master",
    "wp-property/enterprise": "master",
    "wp-property/facebook-tabs": "master",
    "wp-property/feps": "master",
    "wp-property/importer": "master",
    "wp-property/pdf": "master",
    "wp-property/power-tools": "master",
    "wp-property/slideshow": "master",
    "wp-property/supermap": "master",
    "wp-property/term-editor": "master"
  },
  "authors": [
    {
      "name": "Andy Potanin",
      "email": "andy.potanin@usabilitydynamics.com",
      "homepage": "http://usabilitydynamics.com",
      "role": "Developer"
    },
    {
      "name": "Maxim Peshkov",
      "email": "maxim.peshkov@usabilitydynamics.com",
      "homepage": "http://usabilitydynamics.com",
      "role": "Developer"
    },
    {
      "name": "Reid Williams",
      "email": "reid.williams@usabilitydynamics.com",
      "homepage": "http://usabilitydynamics.com",
      "role": "Developer"
    },
    {
      "name": "Anton Korotkov",
      "email": "anton.korotkov@usabilitydynamics.com",
      "homepage": "http://usabilitydynamics.com",
      "role": "Developer"
    }
  ],
  "config": {
    "vendor-dir": "vendor/libraries",
    "bin-dir": "vendor/bin",
    "optimize-autoloader": true,
    "autoloader-suffix": "VendorWPP"
  },
  "extra": {
    "installer-name": "wp-property",
    "text_domain": "wpp",
    "main-file": "wp-property.php",
    "namespace": "UsabilityDynamics\\WPP",
    "class": "UsabilityDynamics\\WPP",
    "installer-paths": {
      "vendor/modules/{$name}":       [ "type:wordpress-module" ],
      "vendor/plugins/{$name}":       [ "type:wordpress-plugin" ],
      "vendor/libraries/{$name}":     [ "type:wordpress-library" ],
      "vendor/themes/{$name}":        [ "type:wordpress-theme" ]
    }
  },
  "license": "GNU"
}
