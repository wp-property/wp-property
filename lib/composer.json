{
  "minimum-stability": "dev",
  "config": {
    "preferred-install": "dist",
    "github-protocols": [ "ssh" ],
    "vendor-dir": "autoload",
    "optimize-autoloader": true
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://repository.usabilitydynamics.com/"
    }
  ],
  "require": {
    "composer/installers": "*",
    "wp-property/wp-property-agents": "dev-feature",
    "wp-property/wp-property-responsive-slideshow": "dev-feature",
    "wp-property/wp-property-supermap": "dev-feature",
    "wp-property/wp-property-terms": "dev-feature"
  },
  "autoload": {
    "classmap": [
      "features/listing-type",
      "features/legacy-meta-attributes",
      "features/elasticsearch",
      "features/taxonomy-wpp-location",
      "features/system-taxonomy",
      "features/field-alias"
    ]
  },
  "extra": {
    "_build": {},
    "installer-paths": {
      "features/{$name}": [
        "type:wordpress-muplugin",
        "type:wordpress-plugin",
        "type:wpackagist-plugin"
      ],
      "etc/{$name}": [
        "type:wordpress-module",
        "type:wordpress-library"
      ]
    }
  }
}
